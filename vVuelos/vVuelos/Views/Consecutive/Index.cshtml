@model IEnumerable<vVuelos.Models.consecutive>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>ID</th>
        <th>
            Descripcion
        </th>
        <th>
            Consecutivo
        </th>
        <th>
            Prefijo
        </th>
        <th>
            Rango Inicial
        </th>
        <th>
            Rango Final
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.value)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.prefix)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.range_int)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.range_out)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Menu","Console","Home")
        </td>
    </tr>
}

</table>
